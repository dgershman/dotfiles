    'prefix': 'class'
    'body': 'class ${1:ClassName} {\n\tvar _${2/\\u/_/g};\n\tfunction $1($2){\n\t\t_${2/:(.*)//g} = ${2/:(.*)//g};$0\n\t}\n}'
