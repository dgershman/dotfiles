    'prefix': 'br'
    'body': 'break;'
  'call':
    'prefix': 'ca'
    'body': 'call(${1:frame});'
  'case':
    'prefix': 'ce'
    'body': 'case ${1:expression} :\n\t${1:statement}'
  'catch':
    'prefix': 'ch'
    'body': 'catch ($1) {\n\t$2\n}'
