    'prefix': 'fs'
    'body': 'fscommand(${1:command}, ${2:paramaters});'
  'function':
    'prefix': 'fn'
    'body': 'function $1($2):$3{\n\t$4\n};'
  'getURL':
    'prefix': 'gu'
    'body': 'getURL($1);'
